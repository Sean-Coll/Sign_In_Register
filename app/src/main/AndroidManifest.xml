<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sign_in_register">

    <queries>
        <package android:name="com.google.android.googlequicksearchbox"/>
    </queries>

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sjog_logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sign_In_Register">

        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LocationSelectionActivity"> </activity>
        <activity android:name=".HelpActivity"> </activity>
        <activity android:name=".LoginVoiceActivity"> </activity>
        <activity android:name=".AdminPage"> </activity>
    </application>

</manifest>